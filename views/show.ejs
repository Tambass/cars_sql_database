<%- include('./partials/header')-%>

<div class="container" style="margin-top: 50px;">
    <div class="row">
        <div class="col-md-12">

            <h1><%=cars.Model%></h1>

            <div class="card mb-3" style="max-width: 540px;">
                <div class="row no-gutters">
                    <div class="col-md-4">
                        <img src="<%=cars.Photo%>" class="card-img" alt="<%=cars.Model%>">
                    </div>
                    <div class="col-md-8">
                        <div class="card-body">
                            <h5 class="card-title">Model : <%=cars.Model%></h5>
                            <p class="card-text">Constructeur : <%=cars.Constructeur%></p>
                            <p class="card-text">Ann√©e : <%=cars.Annee%></p>
                            <p class="card-text">Prix : <%=cars.Prix%></p>
                            <p class="card-text">Energie : <%=cars.Energie%></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<%- include('./partials/footer')-%>